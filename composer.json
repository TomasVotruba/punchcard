{
    "name": "tomasvotruba/punchcard",
    "description": "Lazy and strict way to configure your Laravel projects",
    "keywords": [
        "laravel",
        "config",
        "punchcard"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0",
        "laravel/framework": "^10.1",
        "nikic/php-parser": "^4.15.3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.4",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "symplify/easy-ci": "^11.2",
        "symplify/easy-coding-standard": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\PunchCard\\": ["src", "app"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\PunchCard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "php vendor/bin/phpstan"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomasVotruba\\PunchCard\\PunchCardServiceProvider"
            ],
            "aliases": {
                "PunchCard": "TomasVotruba\\PunchCard\\Facades\\PunchCard"
            }
        }
    }
}
